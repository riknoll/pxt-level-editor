{
    "name": "pxt-editor-extension-sample",
    "version": "0.0.1",
    "description": "A MakeCode editor extension sample",
    "homepage": "https://samelhusseini.github.io/pxt-editor-extension-sample/",
    "scripts": {
        "build:dev": "./node_modules/.bin/webpack --mode development --config webpack.dev.js",
        "build:prod": "./node_modules/.bin/webpack --mode production --config webpack.prod.js",
        "start": "npm run build:dev && webpack-dev-server --open --config webpack.dev.js",
        "predeploy": "npm run build:prod",
        "deploy": "gh-pages -d dist",
        "test": "npm run build:dev"
    },
    "devDependencies": {
        "@fortawesome/fontawesome-free": "^5.9.0",
        "@types/react": "^16.7.7",
        "@types/react-dom": "^16.0.11",
        "autoprefixer": "9.3.1",
        "awesome-typescript-loader": "^5.2.1",
        "eventemitter3": "3.1.0",
        "express": "^4.16.4",
        "gh-pages": "^2.0.1",
        "html-webpack-plugin": "^3.2.0",
        "source-map-loader": "^0.2.4",
        "ts-loader": "^5.3.1",
        "tslint": "5.11.0",
        "tslint-microsoft-contrib": "5.2.1",
        "typescript": "^3.1.6",
        "webpack": "^4.26.1",
        "webpack-cli": "3.1.2",
        "webpack-dev-server": "3.1.10"
    },
    "dependencies": {
        "css-loader": "^3.0.0",
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "style-loader": "^0.23.1"
    }
}